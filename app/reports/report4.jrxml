     "PUBLIC"."PUNTOASEMBLEA" PUNTOASEMBLEA INNER JOIN "PUBLIC"."ASEMBLEA_PUNTOASEMBLEA" ASEMBLEA_PUNTOASEMBLEA ON PUNTOASEMBLEA."ID" = ASEMBLEA_PUNTOASEMBLEA."ORDEDODIA_ID"
