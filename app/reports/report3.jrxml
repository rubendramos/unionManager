	<queryString language="SQL">
		<![CDATA[SELECT
     AFILIADO."ID" AS AFILIADO_ID,
     AFILIADO."CARNETCONFEDERADO" AS AFILIADO_CARNETCONFEDERADO,
     AFILIADO."DATAALTA" AS AFILIADO_DATAALTA,
     AFILIADO."DATABAIXA" AS AFILIADO_DATABAIXA,
     AFILIADO."MILITA" AS AFILIADO_MILITA,
     AFILIADO."OCUPACION_ID" AS AFILIADO_OCUPACION_ID,
     AFILIADO."PERSOA_ID" AS AFILIADO_PERSOA_ID,
     AFILIADO."RAMO_ID" AS AFILIADO_RAMO_ID,
     AFILIADO."SINDICATO_ID" AS AFILIADO_SINDICATO_ID,
     AFILIADO."ORGANISMO_ID" AS AFILIADO_ORGANISMO_ID,
     AFILIADO."ESTADO_ID" AS AFILIADO_ESTADO_ID,
     ASEMBLEA."ID" AS ASEMBLEA_ID,
     ASEMBLEA."DATACELEBRACION" AS ASEMBLEA_DATACELEBRACION,
     ASEMBLEA."TITULO" AS ASEMBLEA_TITULO,
     ASEMBLEA."ORGANISMO_ID" AS ASEMBLEA_ORGANISMO_ID,
     ASEMBLEA."CONVOCANTE_ID" AS ASEMBLEA_CONVOCANTE_ID,
     ASEMBLEA."LUGARCELEBRACION_ID" AS ASEMBLEA_LUGARCELEBRACION_ID,
     ASEMBLEA_PUNTOASEMBLEA."ASEMBLEA_ID" AS ASEMBLEA_PUNTOASEMBLEA_ASEMBLEA_ID,
     ASEMBLEA_PUNTOASEMBLEA."ORDEDODIA_ID" AS ASEMBLEA_PUNTOASEMBLEA_ORDEDODIA_ID,
     PERSOA."ID" AS PERSOA_ID,
     PERSOA."APELIDO1" AS PERSOA_APELIDO1,
     PERSOA."APELIDO2" AS PERSOA_APELIDO2,
     PERSOA."DATANACEMENTO" AS PERSOA_DATANACEMENTO,
     PERSOA."DNI" AS PERSOA_DNI,
     PERSOA."EMAIL" AS PERSOA_EMAIL,
     PERSOA."NOME" AS PERSOA_NOME,
     PERSOA."NOMECOMPLETO" AS PERSOA_NOMECOMPLETO,
     PERSOA."SEXO" AS PERSOA_SEXO,
     PERSOA."SEXO_ID" AS PERSOA_SEXO_ID,
     PERSOA."ENDEREZO_ID" AS PERSOA_ENDEREZO_ID,
     PERSOA."ORGANISMO_ID" AS PERSOA_ORGANISMO_ID,
     PUNTOASEMBLEA."ID" AS PUNTOASEMBLEA_ID,
     PUNTOASEMBLEA."ACRONIMO" AS PUNTOASEMBLEA_ACRONIMO,
     PUNTOASEMBLEA."CIF" AS PUNTOASEMBLEA_CIF,
     PUNTOASEMBLEA."DATAALTA" AS PUNTOASEMBLEA_DATAALTA,
     PUNTOASEMBLEA."DATABAIXA" AS PUNTOASEMBLEA_DATABAIXA,
     PUNTOASEMBLEA."DESCRICION" AS PUNTOASEMBLEA_DESCRICION,
     PUNTOASEMBLEA."NOME" AS PUNTOASEMBLEA_NOME,
     PUNTOASEMBLEA."ORGANISMO_ID" AS PUNTOASEMBLEA_ORGANISMO_ID,
     PUNTOASEMBLEA."ENDEREZO_ID" AS PUNTOASEMBLEA_ENDEREZO_ID,
     PUNTOASEMBLEA."RAMO_ID" AS PUNTOASEMBLEA_RAMO_ID,
     PUNTOASEMBLEA."TIPOORGANISMO_ID" AS PUNTOASEMBLEA_TIPOORGANISMO_ID,
     PUNTOASEMBLEA."APETICIONDE" AS PUNTOASEMBLEA_APETICIONDE,
     PUNTOASEMBLEA."ACORDO" AS PUNTOASEMBLEA_ACORDO,
     PUNTOASEMBLEA_PUNTOASEMBLEA."PUNTOASEMBLEA_ID" AS PUNTOASEMBLEA_PUNTOASEMBLEA_PUNTOASEMBLEA_ID,
     PUNTOASEMBLEA_PUNTOASEMBLEA."ORGANISMOSFILLO_ID" AS PUNTOASEMBLEA_PUNTOASEMBLEA_ORGANISMOSFILLO_ID,
     ENDEREZO."ID" AS ENDEREZO_ID,
     ENDEREZO."CONCELLO" AS ENDEREZO_CONCELLO,
     ENDEREZO."ENDEREZO" AS ENDEREZO_ENDEREZO,
     ENDEREZO."PAIS" AS ENDEREZO_PAIS,
     ENDEREZO."LOCALIDADE_ID" AS ENDEREZO_LOCALIDADE_ID,
     ENDEREZO."ORGANISMO_ID" AS ENDEREZO_ORGANISMO_ID
FROM
     "PUBLIC"."AFILIADO" AFILIADO INNER JOIN "PUBLIC"."ASEMBLEA" ASEMBLEA ON AFILIADO."ID" = ASEMBLEA."CONVOCANTE_ID"
     INNER JOIN "PUBLIC"."PERSOA" PERSOA ON AFILIADO."PERSOA_ID" = PERSOA."ID"
     INNER JOIN "PUBLIC"."ENDEREZO" ENDEREZO ON PERSOA."ENDEREZO_ID" = ENDEREZO."ID"
     AND ENDEREZO."ID" = ASEMBLEA."LUGARCELEBRACION_ID"
     INNER JOIN "PUBLIC"."PUNTOASEMBLEA" PUNTOASEMBLEA ON ENDEREZO."ID" = PUNTOASEMBLEA."ENDEREZO_ID"
     INNER JOIN "PUBLIC"."ASEMBLEA_PUNTOASEMBLEA" ASEMBLEA_PUNTOASEMBLEA ON PUNTOASEMBLEA."ID" = ASEMBLEA_PUNTOASEMBLEA."ORDEDODIA_ID"
     INNER JOIN "PUBLIC"."PUNTOASEMBLEA_PUNTOASEMBLEA" PUNTOASEMBLEA_PUNTOASEMBLEA ON PUNTOASEMBLEA."ID" = PUNTOASEMBLEA_PUNTOASEMBLEA."ORGANISMOSFILLO_ID"
     AND PUNTOASEMBLEA."ID" = PUNTOASEMBLEA_PUNTOASEMBLEA."PUNTOASEMBLEA_ID"]]>
	</queryString>
