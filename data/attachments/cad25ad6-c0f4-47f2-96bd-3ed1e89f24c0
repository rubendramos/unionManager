
--OFERTA ADULTOS--
select distinct ' <LC_OFERTA_ADULTOS_SYN COD_CENTRO="'||o.COD_CENTRO
||'" ANO="'||o.ANO
||'" COD_CICLO="'||o.COD_CICLO
||'" COD_MODULO="'||o.COD_MODULO
||'" COD_ASISTENCIA="'||decode(o.cod_asistencia,1,'P',2,'D')
||'" PRAZAS="'||o.PRAZAS
||'" PRAZAS_DIRECTO="'||o.PRAZAS_DIRECTO
||'" PRAZAS_PROBA="'||o.PRAZAS_PROBA
||'" PRAZAS_DIRECTO_NEE="'||o.PRAZAS_DIRECTO_NEE
||'" PRAZAS_PROBA_NEE="'||o.PRAZAS_PROBA_NEE
||'" ADXUDICADO="'||o.ADXUDICADO
||'" PRAZAS_CUPO3="'||o.PRAZAS_CUPO3
||'" PRAZAS_CUPO3_NEE="'||o.PRAZAS_CUPO3_NEE
||'" LIBERADO="'||o.LIBERADO
||'" DATA_LIBERACION="'||o.DATA_LIBERACION
||'" DATA_PECHE_LIBERACION="'||o.DATA_PECHE_LIBERACION
||'" DATA_LIBERACION_SETEMBRO="'||o.DATA_LIBERACION_SETEMBRO
||'" DATA_PECHE_LIBERACION_SETEMBRO="'||o.DATA_PECHE_LIBERACION_SETEMBRO||'"/>'
 from oferta_adultos o, lc_solicitude_adultos_syn s,lc_detalles_adultos_syn d
 where o.ano=2012
 and s.ano=o.ano
 and d.cod_ciclo=o.cod_ciclo
 and d.cod_centro=o.cod_centro
 and s.codigo=d.cod_solicitude
 and s.cod_tipoAsistencia= decode(o.cod_asistencia,1,'P',2,'D');
 
  
 --SOLICITUDE ADULTOS--
 select '<LC_SOLICITUDE_ADULTOS_SYN CODIGO="'||CODIGO
||'" DNI_ALUMNO="'||DNI_ALUMNO
||'" ANO="'||ANO
||'" GRAO="'||GRAO
||'" COD_CENTRO="'||COD_CENTRO
||'" NOTAMEDIA="'||NOTAMEDIA
||'" COD_ACCESO="'||COD_ACCESO
||'" COD_MODALIDADE="'||COD_MODALIDADE
||'" CONVOCATORIA="'||CONVOCATORIA
||'" MINUSVALIDO="'||MINUSVALIDO
||'" DATASOLICITUDE="'||DATASOLICITUDE
||'" DATATITULO="'||DATATITULO
||'" TIPOSOLICITUDE="'||TIPOSOLICITUDE
||'" ANEXO3="'||ANEXO3
||'" DEPORTISTA="'||DEPORTISTA
||'" DATABAIXA="'||DATABAIXA
||'" PENDENTE_CONVALIDAR="'||PENDENTE_CONVALIDAR
||'" PRESENTA_DNI="'||PRESENTA_DNI
||'" DEPORTISTA_GALEGO="'||DEPORTISTA_GALEGO
||'" DEPORTISTA_NACIONAL="'||DEPORTISTA_NACIONAL
||'" CER_ELITE_GALEGO="'||CER_ELITE_GALEGO
||'" CER_ELITE_NACIONAL="'||CER_ELITE_NACIONAL
||'" CERTIFICADO_MINUSVALIA="'||CERTIFICADO_MINUSVALIA
||'" DICTAMEN_MEDICO="'||DICTAMEN_MEDICO
||'" CERTIFICADO_PAU="'||CERTIFICADO_PAU
||'" CERTIFICADO_PROBA="'||CERTIFICADO_PROBA
||'" EXTRANXEIRO_CUMPRE_REQ="'||EXTRANXEIRO_CUMPRE_REQ
||'" PAIS_TITULO="'||PAIS_TITULO
||'" ESTRANXEIRO_VOLANTE="'||ESTRANXEIRO_VOLANTE
||'" ESTRANXEIRO_HOMOLOGACION="'||ESTRANXEIRO_HOMOLOGACION
||'" ESTRANXEIRO_NOTAS="'||ESTRANXEIRO_NOTAS
||'" PUNTUACION_SECTOR="'||PUNTUACION_SECTOR
||'" PUNTUACION_NON_SECTOR="'||PUNTUACION_NON_SECTOR
||'" PUNTUACION_MODULOS="'||PUNTUACION_MODULOS
||'" PUNTUACION_ACCESO="'||PUNTUACION_ACCESO
||'" CERTIFICADO_SS="'||CERTIFICADO_SS
||'" CERTIFICADO_EMPRES="'||CERTIFICADO_EMPRES
||'" CERTIFICADO_IAE="'||CERTIFICADO_IAE
||'" CERTIFICADO_MODULOS="'||CERTIFICADO_MODULOS
||'" DIAS_SECTOR="'||DIAS_SECTOR
||'" DIAS_OUTRO_SECTOR="'||DIAS_OUTRO_SECTOR
||'" MODULOS_SUPERADOS="'||MODULOS_SUPERADOS
||'" COD_TIPOASISTENCIA="'||COD_TIPOASISTENCIA
||'" DOCUMENTO_MINUSVALIA="'||DOCUMENTO_MINUSVALIA
||'" DOCUMENTO_EXTRANXEIRO="'||DOCUMENTO_EXTRANXEIRO
||'" COD_PERSOA="'||COD_PERSOA
||'" DOCUMENTO_DEPORTISTA="'||DOCUMENTO_DEPORTISTA
||'" ORIXINAL_MINUSVALIA="'||ORIXINAL_MINUSVALIA
||'" ORIXINAL_EXTRANXEIRO="'||ORIXINAL_EXTRANXEIRO
||'" ORIXINAL_DEPORTISTA="'||ORIXINAL_DEPORTISTA
||'" EXCLUIDO="'||EXCLUIDO
||'" EXCLUIBLE="'||EXCLUIBLE ||'"/>'
 from lc_solicitude_adultos_syn where ano=2012;
 
 
--DETALLE SOLICITUDE ADULTOS-- 
 select '<LC_DETALLES_ADULTOS_SYN COD_SOLICITUDE="'||COD_SOLICITUDE
||'" COD_CENTRO="'||d.COD_CENTRO
||'" POSTO="'||d.POSTO
||'" ANO_SOLICITUDE="'||d.ANO_SOLICITUDE
||'" COD_CICLO="'||d.COD_CICLO
||'" COD_MODULO="'||d.COD_MODULO
||'" POSTO_CICLO="'||d.POSTO_CICLO||'"/>'
 from lc_detalles_adultos_syn d, lc_solicitude_adultos_syn s
 where d.cod_solicitude=s.codigo
 and s.ano=2012;
 
 
 --BAREMACION ADULTOS--
 select '<LC_BAREMACION_ADULTOS_SYN COD_SOLICITUDE="'||COD_SOLICITUDE
||'" COD_CENTRO="'||COD_CENTRO
||'" COD_CICLO="'||COD_CICLO
||'" COD_MODULO="'||COD_MODULO
||'" ANO_SOLICITUDE="'||ANO_SOLICITUDE
||'" COD_GRUPO="'||COD_GRUPO
||'" FASE="'||FASE
||'" ESTADO="'||ESTADO
||'" DATA_OPERACION="'||DATA_OPERACION
||'" DEPORTISTA="'||DEPORTISTA||'"/>'
 from lc_baremacion_adultos_syn where ano_solicitude=2012;
 
 
 --FASES ADULTOS
 select '<LC_FASE_ADULTOS_SYN CODIGO="'||CODIGO
||'" ANO="'||ANO
||'" GRAO="'||GRAO
||'" NOME="'||NOME
||'" DATA="'||DATA
||'" ESTADO="'||ESTADO
||'" CONVOCATORIA="'||CONVOCATORIA
||'" COD_TIPOASISTENCIA="'||COD_TIPOASISTENCIA
||'" LIBERADOS="'||LIBERADOS
||'" SETEMBRO="'||SETEMBRO||'"/>'
 from lc_fase_adultos_syn where ano=2012;
 
 --CICLOs
 select distinct '<LC_CICLO_VW_SYN CODIGO="'||c.CODIGO
||'" COD_BOLETIN="'||c.COD_BOLETIN
||'" DESCRIPCION="'||c.DESCRIPCION
||'" GRAO="'||c.GRAO
||'" TIPO="'||c.TIPO
||'" COD_FAMILIA="'||c.COD_FAMILIA ||'"/>'
 from lc_ciclo_vw_syn c,
 lc_oferta_adultos_syn o,lc_detalles_adultos_syn d,lc_solicitude_adultos_syn s
 where o.cod_ciclo=c.codigo
 and o.ano=2012
 and d.cod_ciclo=c.codigo
 and s.codigo=d.cod_solicitude
 and s.ano=o.ano;
 